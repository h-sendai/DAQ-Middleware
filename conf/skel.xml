<?xml version="1.0"?>
<!-- DON'T REMOVE THE ABOVE LINE.                                             -->
<!-- DON'T PUT ANY LINES ABOVE THE 1ST LINE.                                  -->
<!-- Sample config.xml to run Skeleton Component.                             -->
<!-- Please rewrite execPath and confFile (2 places) suitable                 -->
<!-- for your directory structure.                                            -->
<!-- confFile (/path/to/rtc.conf) will be created automatically at this path. -->
<configInfo>
    <daqOperator>
        <hostAddr>127.0.0.1</hostAddr>
    </daqOperator>
    <daqGroups>
        <daqGroup gid="group0">
            <components>
                <component cid="SkeletonComp0">
                    <hostAddr>127.0.0.1</hostAddr>
                    <hostPort>50000</hostPort>
                    <instName>Skeleton0.rtc</instName>
                    <execPath>/home/daq/MyDaq/Skeleton/SkeletonComp</execPath>
                    <confFile>/tmp/daqmw/rtc.conf</confFile>
                    <startOrd>1</startOrd>
                    <inPorts>
                    </inPorts>
                    <outPorts>
                    </outPorts>
                    <params>
                    </params>
                </component>
            </components>
        </daqGroup>
    </daqGroups>
</configInfo>
